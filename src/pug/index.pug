-
	var domain = './'
	var description = 'mail suggest'
	var keywords = 'mail suggest'
	var title = 'mail suggest'
	var date = +new Date()

doctype html
html(lang='ja')
	head
		include include/meta
		include include/title
		include include/css
	body
		.l-main
			label.p-form__label(for="email") ✉️ Email
			.p-suggest__container
				input.p-form__input#email(autocomplete='off' type='email' data-trigger-suggest)
				ul.p-suggest__result#auto-list(style='display:none;' data-target-suggest)
		include include/js